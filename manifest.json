{
    "manifest_version": 4,
    "type": "theme",
    "name": "MomotalkNTQQ 主题",
    "slug": "momotalk_theme",
    "description": "低仿 MomotalkNTQQ 风格的 QQNT 主题 （基于Telegram-Theme修改而成）",
    "version": "4.0.1",
    "icon": "./icon.png",
    "authors": [
        {
            "name": "Dax233",
            "link": "https://github.com/Dax233"
        }
    ],
    "platform": ["win32", "linux", "darwin"],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    },
    "repository": {
        "repo": "Dax233/MomoTalkNTQQ-Theme",
        "branch": "v4",
        "release": {
            "tag": "latest",
            "file": "MomoTalkNTQQ-Theme.zip"
        }
    }
}
